<nav aria-label="Search result pages" class="ds_pagination">
    <ul class="ds_pagination__list">
        {% for page in pages %}
            <li class="ds_pagination__item" {% if page.isEllipsis %}aria-hidden="true"{% endif %}>

                

                {% if page.isLink %}
                    {% if page.isPrevious %}
                    <a aria-label="Previous page" class="ds_pagination__link  ds_pagination__link--text  ds_pagination__link--icon" data-search="pagination-{{page.dataName}}" data-start="{{page.start}}" href="#">
                        <svg class="ds_icon" aria-hidden="true" role="img">
                            <use href="{{ page.iconPath | safe }}#chevron_left"></use>
                        </svg>
                        <span class="ds_pagination__link-label">{{page.displayText | safe}}</span>
                    </a>
                    {% elseif page.isNext %}
                    <a aria-label="Next page" class="ds_pagination__link  ds_pagination__link--text  ds_pagination__link--icon" data-search="pagination-{{page.dataName}}" data-start="{{page.start}}" href="#">
                        <span class="ds_pagination__link-label">{{page.displayText | safe}}</span>
                        <svg class="ds_icon" aria-hidden="true" role="img">
                            <use href="{{ page.iconPath | safe }}#chevron_right"></use>
                        </svg>
                    </a>
                    {% else %}
                    <a {% if page.isCurrentPage %}aria-current="page"{% endif %} aria-label="{% if page.isLast %}Last page, page{% else %}Page{% endif %} {{page.displayText | safe}}" href="#" data-search="pagination-{{page.dataName}}" class="ds_pagination__link  {% if page.isCurrentPage %}ds_current{% endif %}" data-start="{{page.start}}">{{page.displayText | safe}}</a>
                    {% endif %}
                {% else %}
                    <span class="ds_pagination__link {% if page.isCurrentPage %} ds_current{% endif %} {% if page.isEllipsis %} ds_pagination__link--ellipsis{% endif %}">{{page.displayText | safe}}</span>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
</nav>
